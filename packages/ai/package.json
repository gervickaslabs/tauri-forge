{
  "name": "@repo/ai",
  "version": "0.0.1",
  "private": true,
  "files": [
    "src"
  ],
  "exports": {
    "./*": "./src/**.(js|ts|jsx|tsx)",
    "./components/*": "./src/components/*.js",
    "./hooks/*": "./src/hooks/*.js",
    "./utils/*": "./src/utils/*.js"
  },
  "scripts": {
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "build": "tsc",
    "check-types": "tsc --noEmit",
    "dev": "tsc --watch",
    "lint": "eslint src --max-warnings 0"
  },
  "peerDependencies": {
    "react": "^19"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@ai-sdk/react": "^1.2.12",
    "@repo/ui": "workspace:*",
    "ai": "^4.3.16",
    "react": "^19.1.0",
    "react-markdown": "^10.1.0",
    "typescript": "5.8.3",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "22.15.21",
    "@types/react": "19.1.5",
    "@types/react-dom": "^19.1.5",
    "eslint": "^9.31.0"
  }
}
